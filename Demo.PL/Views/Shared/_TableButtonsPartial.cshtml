@model object
<td><a  asp-action="Details" class="btn btn-success" asp-route-id="@Model"><i class="fas fa-eye"></i> Details</a></td>
<td><a  asp-action="Update" asp-route-id="@Model" class="btn btn-warning"><i class="fas fa-edit"></i> Update</a></td>
@*<td><a  asp-action="Delete" asp-route-id="@Model" class="btn btn-danger"><i class="fas fa-trash"></i> Delete</a></td>*@
<td><button type="button" class="btn btn-danger" onclick="deleteItem('@Model',this)"><i class="fas fa-trash"></i> Delete</button></td>
